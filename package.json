{
  "name": "quiriquiri",
  "version": "2.0.10",
  "description": "QuiriQuiri: a Twitter client",
  "repository": {
    "type": "git",
    "url": "https://https://github.com/conradoplg/quiriquiri.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter spec -r test/setup.js",
    "start": "electron --disable-gpu .",
    "format": "find . -path ./node_modules -prune -o -name '*.js' -print | xargs js-beautify -r -n",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64"
  },
  "author": "Conrado P. L. Gouvea",
  "license": "MIT",
  "dependencies": {
    "electron-debug": "^2.2.0",
    "electron-window-state": "^5.0.3",
    "featherlight": "^1.7.14",
    "is-in-viewport": "^2.4.2",
    "jquery": "^3.4.1",
    "jquery-ui-dist": "^1.12.1",
    "jsdom": "^9.5.0",
    "mashape-oauth": "^0.1.71",
    "sinon": "^1.17.6",
    "twemoji": "^12.1.2",
    "twit": "^2.2.5",
    "twitter-text": "^1.14.0",
    "winston": "^3.2.1",
    "zpad": "^0.5.0"
  },
  "devDependencies": {
    "electron": "^6.0.5",
    "electron-builder": "^21.2.0",
    "mocha": "^6.2.0"
  },
  "build": {
    "appId": "net.cryptoland.quiriquiri"
  }
}
